# Winterdienst Tracker - Environment Variables
# Copy this file to .env and fill in the values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Company Branding (set these in Vercel to customize for your company)
VITE_COMPANY_NAME=Your Company
VITE_COMPANY_SHORT_NAME=YC
VITE_COMPANY_SUBTITLE=Winter Service
VITE_COMPANY_FOOTER=Winter Service - Work Hours Report
VITE_SERVICE_NAME=Winterdienst

# Feature Flags (set to "true" to enable)
VITE_ENABLE_BG_FILTER=false

# Docker Configuration (optional)
PORT=3000
